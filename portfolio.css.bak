header{
	text-align:center;
	font-size: large;
	margin:auto;
	width:1150px;
	font-family:trebuchet;	
}
nav{
	font-family:"Snell Roundhand";
	background-color:black;
	padding:6px;
	border-radius: 10px;
	margin:0;
	width:auto;

}
nav>a{
	padding-left:0;
	padding-right:0;
	margin:0;
}
a{
	color:white;
	text-decoration:none;
	padding: 1em;
	margin: 1em;
}
body{
	width: 1200px;
	margin: auto;
	text-align:center;
}
main{
	background-color: lightgray;
	border-radius: 6px;
}
h2{
	text-align:center;
}
img{
	max-height :250px;
	float : none;
}
p{
	text-align:center;
}
p>a{
	color:blue;
	text-decoration: underline;
}
li>a{
	color:blue;
	text-decoration: underline;
}